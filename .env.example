# AWS Configuration
# Copy this file to .env and fill in your values

# AWS Profile to use (must be configured with 'aws configure --profile <name>')
AWS_PROFILE=exalm

# AWS Region (us-east-1 is required for Nova Sonic model)
AWS_DEFAULT_REGION=us-east-1

# Voice Assistant Configuration
BYPASS_TOOL_CONSENT=true

# Model Configuration
CLAUDE_MODEL_ID=anthropic.claude-3-haiku-20240307-v1:0
NOVA_SONIC_MODEL_ID=amazon.nova-sonic-v1:0

# Optional: Override voice model (default: matthew)
# VOICE_MODEL=matthew

# Optional: Override server port (default: 8080)
# SERVER_PORT=8080

# Optional: Override frontend URL (default: http://localhost:3000)
# FRONTEND_URL=http://localhost:3000